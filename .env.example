# Kubernetes configuration
# For development/testing without a real cluster - set to "true"
VITE_USE_MOCK_K8S_DATA=false
VITE_USE_MOCK_K8S_DATA_FALLBACK=false

# Supabase configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Kubernetes API proxy settings
VITE_K8S_PROXY_URL=http://localhost:3001
VITE_API_PROXY_URL=http://localhost:3001

# AWS Region for SDK operations if not specified in kubeconfig
VITE_AWS_REGION=us-east-1

# AWS credentials (using placeholder values for security)
# Replace these with your actual credentials when running locally, but DO NOT commit them
VITE_AWS_ACCESS_KEY_ID=YOUR_ACCESS_KEY_ID_HERE
VITE_AWS_SECRET_ACCESS_KEY=YOUR_SECRET_ACCESS_KEY_HERE
# VITE_AWS_SESSION_TOKEN=YOUR_SESSION_TOKEN_HERE  # Uncomment if using temporary credentials

# K8s token - This will be generated automatically by the proxy server now
VITE_K8S_AUTH_TOKEN=YOUR_K8S_AUTH_TOKEN_HERE

# DynamoDB configuration
# Replace Supabase with AWS Cognito and DynamoDB
VITE_COGNITO_USER_POOL_ID=YOUR_COGNITO_USER_POOL_ID_HERE
VITE_COGNITO_CLIENT_ID=YOUR_COGNITO_CLIENT_ID_HERE
# DynamoDB tables - make sure to create these tables in your AWS account
# Table: clusters (partition key: id, sort key: none, GSI: owner_id-index)
# Table: checkpoints (partition key: id, sort key: none, GSI: owner_id-index)

# Server configuration
PORT=3001
NODE_ENV=development

# Optional: Path to kubeconfig file (can also be uploaded via UI)
# KUBECONFIG_PATH=/path/to/your/kubeconfig