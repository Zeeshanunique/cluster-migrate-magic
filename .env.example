# Kubernetes configuration
# For development/testing without a real cluster - set to "true"
VITE_USE_MOCK_K8S_DATA=false

# Supabase configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Kubernetes API proxy settings
VITE_K8S_PROXY_URL=http://localhost:3001
VITE_API_PROXY_URL=http://localhost:3001

# AWS credentials (required for AWS EKS integration)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Server configuration
PORT=3001
NODE_ENV=development

# Optional: Path to kubeconfig file (can also be uploaded via UI)
# KUBECONFIG_PATH=/path/to/your/kubeconfig